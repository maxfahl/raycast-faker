{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "faker",
  "title": "Faker",
  "description": "Generate different kinds of placeholder data. Such as names, phone numbers and emails etc.",
  "icon": "command-icon.png",
  "author": "fahl",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "fake",
      "title": "Fake",
      "subtitle": "Fake random data",
      "description": "Generate a random name, email or phone number etc.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@faker-js/faker": "^6.1.2",
    "@raycast/api": "^1.31.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
